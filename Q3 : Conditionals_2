//print the food item and it's price on separate lines
#include<stdio.h>
int main()
{
    int num;
    printf("Enter a number between 1 and 5 : ");
    scanf("%d",&num);
    switch(num)
    {
        case 1 :
        printf("Food Item - Pizza\nPrice - Rs.399");
        break;

        case 2 :
        printf("Food Item - French Fries\nPrice - Rs.70");
        break;

        case 3 :
        printf("Food Item - Burger\nPrice - Rs.120");
        break;

        case 4 :
        printf("Food Item - Noodles\nPrice - Rs.99");
        break;

        case 5 : 
        printf("Food Item - Dhosa\nPrice - Rs.120");
        break;

        default:
        printf("Please enter a valid choice ");

    }
    return 0;
}
